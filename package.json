{
    "name": "under-30",
    "displayName": "Under 30 - Function Length Checker and Line Counter",
    "description": "Displays number of selected lines in the status bar and alerts when a detected function exceeds 30 liens.",
    "version": "1.0.0",
  	"publisher": "Dan Glorioso",
    "icon": "icon.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/danglorioso/under-30"
    },
    "engines": {
      "vscode": "^1.73.0"
    },
    "categories": [
      "Other"
    ],
    "contributes": {
      "commands": [
        {
          "command": "under30.checkfunctionlength",
          "title": "Check Function Length of All Functions in This File"
        }
      ]
    },
    "activationEvents": [
      "*"
    ],
    "main": "./out/extension.js",
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "lint": "eslint \"src/**/*.ts\"",
      "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
      "@types/vscode": "^1.73.0",
      "@typescript-eslint/eslint-plugin": "^6.7.0",
      "@typescript-eslint/parser": "^6.7.0",
      "eslint": "^8.26.0",
      "typescript": "^5.4.2"
    }
  }
